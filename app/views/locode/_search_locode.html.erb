<div class="row" style="float:right">
 <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div id="navbarSupportedContent">
          <%= form_with(method: "get", local: true) do |f| %>
            <div class="form-row">
              <div class="col-md-3 mb-3">
                <label for="validationServer013">Search Code</label>
                <%= f.text_field(:code, value: params[:code],placeholder: "LO/COde",class:"form-control") %>
              </div>
              <div class="col-md-3 mb-3">
                <label for="validationServer013">Search Name</label>
                <%= f.text_field(:name, value: params[:name],placeholder: "Type Name",class:"form-control") %>
              </div>
              <div class="col-md-3 mb-3">
                  <label for="validationServer013">Nearest HUB</label>
                  <%= f.text_field(:origin, value: params[:origin],placeholder: "Type Name",class:"form-control",id: "typeahead") %>
              </div>
              <div class="col-md-3 mb-3">
              <label for="validationServer013" style="display: block">&nbsp;</label>
                <button class="btn btn-primary" type="submit">Search</button>
              </div>
            </div>
          <% end%>
        </div>
      </nav>
</div>
